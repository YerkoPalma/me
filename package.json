{
  "name": "website",
  "version": "1.0.2",
  "description": "Awesome site created with choo-genesis",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "standard --verbose | snazzy",
    "start": "budo -p 8080 -H 0.0.0.0 index.js:bundle.js -- -t sheetify",
    "build": "NODE_ENV=production bankai build --optimize dist/ && cp 404.html service-worker.js CNAME favicon.ico dist/",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "standard": {
    "ignore": [
      "service-worker.js",
      "dist/"
    ]
  },
  "author": "Yerko Palma",
  "license": "MIT",
  "dependencies": {
    "bel": "^5.1.3",
    "choo": "^6.5.0",
    "marli": "^1.1.0",
    "sheetify": "^6.2.0",
    "tachyons": "^4.8.1"
  },
  "devDependencies": {
    "budo": "^10.0.4",
    "gh-pages": "^1.0.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  }
}
