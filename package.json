{
  "name": "website",
  "version": "1.0.0",
  "description": "Awesome site created with choo-genesis",
  "main": "src/app.js",
  "scripts": {
    "copy": "mkdir -p dist/src && cp -R src/assets dist/src && mkdir -p dist/assets/style && cp CNAME favicon.ico index.html dist/ && cp src/assets/style/main.css dist/assets/style",
    "dev:less": "lessc src/assets/style/main.less src/assets/style/main.css",
    "copy-sh": "cp dist/index.html dist/200.html",
    "dev:js": "budo src/app.js --open --live --pushstate --host 0.0.0.0 --port 8080 --",
    "dev": "gazer -p 'src/assets/style/**/*.less' npm run dev:less & npm run dev:js",
    "test": "standard --verbose | snazzy",
    "build:js": "NODE_ENV=production browserify -e src/app.js -o dist/app.js -t envify -t sheetify/transform -g es2040 -g uglifyify | uglifyjs",
    "build": "npm run copy && npm run build:js",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "choo",
    "generator",
    "scaffold"
  ],
  "browserify": {
    "transform": [
      "sheetify/transform",
      "es2040"
    ]
  },
  "standard": {
    "ignore": [
      "service-worker.js",
      "dist/"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "choo": "^3.3.0",
    "choo-offline": "^0.1.3",
    "granim": "^1.0.6",
    "js-md5": "^0.4.1",
    "tachyons": "^4.5.3"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-es2040": "^1.1.1",
    "browserify": "^13.1.0",
    "budo": "^9.2.1",
    "choo-log": "^2.0.0",
    "choo-test": "0.0.5",
    "envify": "^3.4.1",
    "es2040": "^1.2.4",
    "gazer": "^1.0.0",
    "gh-pages": "^0.11.0",
    "insert-css": "^1.0.0",
    "is-reachable": "^1.3.0",
    "less": "^2.7.1",
    "mochify": "^2.18.1",
    "sheetify": "^5.1.1",
    "snazzy": "^5.0.0",
    "standard": "^8.3.0",
    "sw-precache": "^4.1.0",
    "uglifyify": "^3.0.3",
    "uglifyjs": "^2.4.10"
  }
}
